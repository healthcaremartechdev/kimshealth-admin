{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false
    },
    "specialities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speciality.speciality"
    },
    "date": {
      "type": "date",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaSection": {
      "type": "component",
      "repeatable": false,
      "component": "component.meta-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "diseases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::disease.disease"
    },
    "procedures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::procedure.procedure"
    },
    "details": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "locations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::location.location"
    },
    "blogCategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-category.blog-category"
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::doctor-detail.doctor-detail"
    },
    "shortDetails": {
      "type": "text"
    },
    "manageAppearance": {
      "type": "component",
      "repeatable": false,
      "component": "component.minimal-appearance"
    },
    "blogSection": {
      "type": "component",
      "repeatable": false,
      "component": "component.blog-section"
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "doctor_details",
  "info": {
    "singularName": "doctor-detail",
    "pluralName": "doctor-details",
    "displayName": "Doctor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "doctorQualification": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "doctorDesignation": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "workExperience": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "areaOfExpertise": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "educationAndTraning": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "membership": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "awards": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "researchAndPublication": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "doctorTalk": {
      "type": "component",
      "repeatable": false,
      "component": "component.doctor-talk",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "blogSection": {
      "type": "component",
      "repeatable": false,
      "component": "component.blog-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "manageAppearance": {
      "type": "component",
      "repeatable": false,
      "component": "component.minimal-appearance",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaSection": {
      "type": "component",
      "repeatable": false,
      "component": "component.meta-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "doctorImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "specialities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speciality.speciality"
    },
    "procedures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::procedure.procedure"
    },
    "diseases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::disease.disease"
    },
    "hospitals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::hospital.hospital"
    },
    "locations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::location.location"
    },
    "doctorGender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Others"
      ]
    },
    "languagesKnown": {
      "type": "richtext"
    },
    "appointmentAvailable": {
      "type": "boolean",
      "default": true
    },
    "teleConsultationAvailable": {
      "type": "boolean",
      "default": false
    },
    "salutation": {
      "type": "string",
      "default": "Dr."
    }
  }
}

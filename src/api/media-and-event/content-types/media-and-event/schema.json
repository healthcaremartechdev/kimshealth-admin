{
  "kind": "collectionType",
  "collectionName": "media_and_events",
  "info": {
    "singularName": "media-and-event",
    "pluralName": "media-and-events",
    "displayName": "Media and Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "shortDetails": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "details": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "manageAppearance": {
      "type": "component",
      "repeatable": false,
      "component": "component.minimal-appearance",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaSection": {
      "type": "component",
      "repeatable": false,
      "component": "component.meta-section",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "locations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::location.location"
    },
    "speaker": {
      "type": "component",
      "repeatable": false,
      "component": "component.speaker-section"
    },
    "mediaSection": {
      "type": "component",
      "repeatable": false,
      "component": "component.media-and-events-section"
    },
    "hospitals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::hospital.hospital"
    },
    "specialities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speciality.speciality"
    }
  }
}
